{
  "name": "discord-event-bot-modular",
  "version": "4.0.0",
  "description": "Modular Discord bot for managing events with Google Calendar integration, Twitch/YouTube monitoring, custom signup roles, and web interface",
  "main": "src/bot.js",
  "scripts": {
    "start": "node src/bot.js",
    "web": "node web-server.js",
    "migrate": "node scripts/migrate-to-database.js",
    "start:all": "node start-all.js",
    "dev": "nodemon src/bot.js",
    "dev:web": "nodemon web-server.js",
    "dev:all": "concurrently \"nodemon src/bot.js\" \"nodemon web-server.js\"",
    "pm2:start": "pm2 start src/bot.js --name discord-event-bot && pm2 start web-server.js --name web-server && pm2 save",
    "pm2:stop": "pm2 stop discord-event-bot web-server",
    "pm2:restart": "pm2 restart discord-event-bot web-server --update-env",
    "pm2:delete": "pm2 delete discord-event-bot web-server",
    "pm2:logs": "pm2 logs",
    "pm2:status": "pm2 list",
    "pm2:monit": "pm2 monit",
    "install:pm2": "npm install -g pm2"
  },
  "keywords": [
    "discord",
    "bot",
    "events",
    "calendar",
    "google-calendar",
    "twitch",
    "youtube",
    "streaming",
    "notifications",
    "web-interface",
    "modular",
    "database",
    "sqlite"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "discord.js": "^14.14.1",
    "googleapis": "^130.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "axios": "^1.6.2",
    "xml2js": "^0.6.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.7",
	"googleapis": "^144.0.0"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  },
  "optionalDependencies": {
    "pm2": "^5.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}